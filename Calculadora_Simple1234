clear; clc;

disp ('Buenas, persona promedio')
disp('A continuacion ')
UNO= input('Ingrese un numero : ');
DOS= input('Ingrese un numero : ');
TRES= input('Ingrese un numero : ');
CUATRO= input('Ingrese un numero : ');
CINCO= input('Ingrese un numero : ');
clc;

disp('1.Suma  2.Resta 3.Multiplicacion 4. Division')
respuesta= input('Que opcion desea realizar? (elija el numero correspondiente); ');

switch respuesta
  case 1
      res= UNO+DOS+TRES+CUATRO+CINCO;
      fprintf('El resultado de la operacion es: %i' ,res) 
  case 2 
     res= UNO-DOS-TRES-CUATRO-CINCO;
     %Esta restado arbitrariamente ya que no se especifico en el problema
     fprintf('El resultado de la operacion es: %i' ,res) 
  case 3
     res=UNO*DOS*TRES*CUATRO*CINCO;
     fprintf('El resultado de la operacion es: %i ',res) 
case 4
  %Esta dividido arbitrariamente ya que no se especifico en el problema
     res=UNO+DOS+TRES/CUATRO+CINCO;
     fprintf('El resultado de la operacion es: %i ',res) 
end
   fprintf(' \n Gracias por participar c: ')
 
